# Team Kyoto

oneA = []
for x in range(5):
    oneA.append(str(x*22).zfill(2))
print(oneA)

oneB = [str(x*22).zfill(2) for x in range(5)]
print(oneB)

twoA = []
for x in range(5):
    twoA.append(x*10 + 7)
print(twoA)

twoB = [x*10+7 for x in range(5)]
print(twoB)

threeA = []
for x in range(9):
    if x > 5:
        threeA.append(x%3*2)
    elif x > 2:
        threeA.append(x%3)
    else:
        threeA.append(0)
print(threeA)

twoB = [x%3*2 if x>5 else x%3 if x>2 else 0 for x in range(9)]
print(twoB)
